    <Resources>
        <Resource pid="hdl:20.500.11752/rootcorpora">
            <Title xml:lang="it">Corpora offerti da ILC4CLARIN</Title>
            <Title xml:lang="en">The ILC4CLARIN corpora</Title>
             <Title xml:lang="it">Corpora offerti da ILC4CLARIN</Title>
            <Title xml:lang="en">ILC4CLARIN offered corpora</Title>
      
            <LandingPageURI>http://ilc4clarin.ilc.cnr.it/en/resources</LandingPageURI>
            <Languages>
                <Language>ita</Language>
            </Languages>
            <AvailableDataViews ref="hits adv"/>
            <AvailableLayers ref="word lemma pos"/>
            <Resources>
                <Resource pid="hdl:20.500.11752/it-treebanks"> <!-- hdl:20.500.11752/parole -->
                    <Title xml:lang="it">ILC4CLARIN Treebank italiane.</Title>
                    <Title xml:lang="en">ILC4CLARIN Italian Treebanks.</Title>
                    <Description xml:lang="it">ILC4CLARIN Collezione di diverse treebank italiane. Estratte dal progetto UD2.3.</Description>
                    <Description xml:lang="en">ILC4CLARIN Itralian Treebanks collection. Extracted from UD2.3 project.</Description>
                    <LandingPageURI>http://ilc4clarin.ilc.cnr.it/en/it-treebanks</LandingPageURI>
                    <Languages>
                        <Language>ita</Language>
                    </Languages>
                    <AvailableDataViews ref="hits adv"/>
                    <AvailableLayers ref="word lemma pos"/>
                    <Resources>
                        <Resource pid="hdl:20.500.11752/ud-italian-istd">
                            <Title xml:lang="it">Corpus UD_Italian-ISDT</Title>
                            <Title xml:lang="en">The UD_Italian-ISDT corpus</Title>
                            <Description xml:lang="it">Il corpus italiano annotato secondo lo schema UD (originalmente da ISDT (Italian Stanford Dependency Treebank)).</Description>
                            <Description xml:lang="en">The Italian corpus annotated according to the UD annotation scheme (from ISDT (Italian Stanford Dependency Treebank)).</Description>
                            <LandingPageURI>http://ilc4clarin.ilc.cnr.it/en/ud-italian-istd</LandingPageURI>
                            <Languages>
                                <Language>ita</Language>
                            </Languages>
                            <AvailableDataViews ref="hits adv"/>
                            <AvailableLayers ref="word lemma pos"/>
                        </Resource>
                        <Resource pid="hdl:20.500.11752/ud-italian-pud">
                            <Title xml:lang="it">Corpus UD_Italian-PUD</Title>
                            <Title xml:lang="en">The UD_Italian-PUD corpus</Title>
                            <Description xml:lang="it">Il corpus italiano come parte della Treebank parallela PUD.</Description>
                            <Description xml:lang="en">The Italian corpus as part of the Parallel Universal Dependencies (PUD) Treebank.</Description>
                            <LandingPageURI>http://ilc4clarin.ilc.cnr.it/en/ud-italian-pud</LandingPageURI>
                            <Languages>
                                <Language>ita</Language>
                            </Languages>
                            <AvailableDataViews ref="hits adv"/>
                            <AvailableLayers ref="word lemma pos"/>
                        </Resource>
                        <Resource pid="hdl:20.500.11752/ud-italian-partut">
                            <Title xml:lang="it">Corpus UD_Italian-ParTUT</Title>
                            <Title xml:lang="en">The UD_Italian-ParTUT corpus</Title>
                            <Description xml:lang="it">UD_Italian-ParTUT è una conversione di una Treebank parallela sviluppata presso l'Università di Torino.</Description>
                            <Description xml:lang="en">UD_Italian-ParTUT is a conversion of a multilingual parallel treebank developed at the University of Turin.</Description>
                            <LandingPageURI>http://ilc4clarin.ilc.cnr.it/en/ud-italian-partut</LandingPageURI>
                            <Languages>
                                <Language>ita</Language>
                            </Languages>
                            <AvailableDataViews ref="hits adv"/>
                            <AvailableLayers ref="word lemma pos"/>
                        </Resource>
                        <Resource pid="hdl:20.500.11752/ud-italian-postwita">
                            <Title xml:lang="it">Corpus PoSTWITA-UD</Title>
                            <Title xml:lang="en">The PoSTWITA-UD corpus</Title>
                            <Description xml:lang="it">PoSTWITA-UD è una collezione di tweet italiani annotati in UD.</Description>
                            <Description xml:lang="en">PoSTWITA-UD is a collection of Italian tweets annotated in UD.</Description>
                            <LandingPageURI>http://ilc4clarin.ilc.cnr.it/en/ud-italian-postwita</LandingPageURI>
                            <Languages>
                                <Language>ita</Language>
                            </Languages>
                            <AvailableDataViews ref="hits adv"/>
                            <AvailableLayers ref="word lemma pos"/>
                        </Resource>
                    </Resources>
                </Resource>
